[binaries]
c = '${ARCH}-${TRIPLE}-gcc'
cpp = '${ARCH}-${TRIPLE}-g++'
ar = '${ARCH}-${TRIPLE}-gcc-ar'
nm = '${ARCH}-${TRIPLE}-nm'
objcopy = '${ARCH}-${TRIPLE}-objcopy'
ranlib = '${ARCH}-${TRIPLE}-ranlib'
strip = '${ARCH}-${TRIPLE}-strip'
pkg-config = '${ARCH}-${TRIPLE}-pkg-config'
llvm-config = '/base_dir/support/llvm-config'

[host_machine]
system = 'menix'
cpu_family = '${ARCH}'
cpu = '${ARCH}'
endian = 'little'

[built-in options]
libdir = 'lib'
